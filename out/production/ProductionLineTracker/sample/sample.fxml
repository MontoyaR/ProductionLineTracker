                <Button layoutX="171.0" layoutY="133.0" mnemonicParsing="false" onMouseClicked="#Println" text="Button" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="Production Log">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <Button layoutX="171.0" layoutY="133.0" mnemonicParsing="false" onMouseClicked="#Println" text="Button" />
              </children>
            </AnchorPane>
